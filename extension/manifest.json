{
  "name": "Twilio Video Screen sharing2",
  "description":
    "An extension to allow you to test Twilio Video screen sharing locally.",
  "version": "0.1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["extension.js"],
    "persistent": false
  },
  "externally_connectable": {
    "matches": ["*://chat.linchaoqun.com/*","*://localhost/*"]
  },
  "permissions": ["desktopCapture"]
}
